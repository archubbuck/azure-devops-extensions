{
  "manifestVersion": 1,
  "id": "better-logs",
  "publisher": "notification-hub-publisher",
  "version": "10.0.0",
  "name": "Better Logs",
  "description": "Centralized log management hub for Azure DevOps extensions. Captures and displays logs from this extension in a unified interface and serves as a template for adding centralized logging to other extensions.",
  "public": false,
  "categories": [
    "Azure Boards",
    "Azure Repos",
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "apps/better-logs/dist/favicon.ico"
  },
  "contributions": [
    {
      "id": "better-logs-hub",
      "type": "ms.vss-web.hub",
      "description": "Better Logs centralized logging hub",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Logs",
        "uri": "apps/better-logs/dist/index.html"
      }
    }
  ],
  "scopes": [],
  "files": [
    {
      "path": "apps/better-logs/dist",
      "addressable": true
    }
  ],
  "metadata": {
    "lastVersionCommit": "",
    "lastVersionUpdate": ""
  }
}
